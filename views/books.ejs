<% include partials/header %>
<h1><%= title %></h1>
<table>
    <thead>
        <tr>
        <th>Title</th>
        <th>Author</th>
        <th>Genre</th>
        <th>Pages</th>
        <th>URL</th>
        </tr>
    </thead>
    <tbody><% for (let i = 0; i < books.length; i++) { %>        
      <tr>
        <td><%= books[i].title %></td>
        <td><%= books[i].author %></td>
        <td><%= books[i].genre %></td>
        <td><%= books[i].pages %></td>
        <td><a href="<%= books[i].url %>"><%= books[i].url %></a></td>        
      </tr>   
      <% } %></tbody>    
</table>
<% include partials/footer %>   