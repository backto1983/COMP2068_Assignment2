<% include partials/header %>
<main class="center" style="margin-left: 100px; margin-right: 100px">
    <h1 style="color:#000000"><%= title %></h1>
    <table class="responsive-table centered striped highlight" style="color:#000000">
        <thead>
            <tr>
            <th>Title</th>
            <th>Author</th>
            <th>Genre</th>
            <th>Pages</th>
            <th>URL</th>
            </tr>
        </thead>
        <tbody><% for (let i = 0; i < books.length; i++) { %>        
        <tr>
            <td><%= books[i].title %></td>
            <td><%= books[i].author %></td>
            <td><%= books[i].genre %></td>
            <td><%= books[i].pages %></td>
            <td><a href="<%= books[i].url %>"><%= books[i].url %></a></td>        
        </tr>   
        <% } %></tbody>    
    </table>
</main>
<% include partials/footer %>   