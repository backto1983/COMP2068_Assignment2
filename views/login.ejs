<% include partials/header %>
<main class="center">
    <h1><%= title %></h1>
    <form class="container" style="margin-left: 30%; margin-right: 30%" method="POST"> 
        <div class="row">
            <div class="input-field col s7" style="margin-bottom: 0">
                <label for="username">Username</label>
                <input type="email" id="username" name="username" required>               
            </div>
        </div>
        <div class="row">
            <div class="input-field col s7" style="margin-bottom: 0">
                <label for="password">Password</label>
                <input type="password" id="password" name="password" required>                
            </div>
        </div>     
    </form>
    <div><a class="waves-effect waves-light btn-small black" href="/login">Login</a></div>
</main>
<% include partials/footer %>
